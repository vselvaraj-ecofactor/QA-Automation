 <suite name="Insite Login UI Tests">

	<test name="Login UI" preserve-order="true">
		<groups>
			<run>
				<include name="ui" />
			</run>
		</groups>
		<classes>
			<class name="com.ecofactor.qa.automation.insite.regression.LoginTest" />
		</classes>
	</test>

	<listeners>
		<listener class-name="com.ecofactor.qa.automation.util.CustomReport" />
		<listener class-name="com.ecofactor.qa.automation.util.TestLogListener" />
		<listener class-name="com.ecofactor.qa.automation.util.TestResultSetUp" />
		<listener class-name="com.ecofactor.qa.automation.util.test.JobValidator" />
	</listeners>

</suite>

