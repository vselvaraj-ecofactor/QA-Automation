<?xml version="1.0" encoding="UTF-8"?>
<suite name="OpsScript">

	<test name="OPS Script Execution" preserve-order="true">
		<groups>
			<run>
				<include name="sanity" />
			</run>
		</groups>
		<classes>
			<class
				name="com.ecofactor.qa.automation.opsscript.regression.OpsScriptTest" />
		</classes>
	</test>

	<listeners>
		<listener class-name="com.ecofactor.qa.automation.util.TestLogListener"/>
		<listener class-name="com.ecofactor.qa.automation.util.TestResultSetUp" />
	</listeners>

</suite>